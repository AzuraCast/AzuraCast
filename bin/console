#!/usr/bin/env php
<?php

declare(strict_types=1);

error_reporting(E_ALL & ~E_NOTICE & ~E_STRICT);
ini_set('display_errors', '1');

require dirname(__DIR__) . '/vendor/autoload.php';

$cli = App\AppFactory::createCli(
        [
                App\Environment::BASE_DIR => dirname(__DIR__),
        ]
);
$cli->run();
